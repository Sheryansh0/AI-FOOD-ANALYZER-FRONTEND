{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://food-analyzer-backend-sea.southeastasia.azurecontainer.io:8000/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
